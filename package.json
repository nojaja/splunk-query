{
  "name": "splunk-query-cli",
  "version": "0.1.0",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts && npx depcruise src",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --config=jest.config.js --coverage",
    "build": "tsc -p tsconfig.json",
    "docs": "typedoc --options typedoc.json",
    "e2e:up": "docker-compose -f test/e2e/docker-compose.yml up -d",
    "e2e:down": "docker-compose -f test/e2e/docker-compose.yml down"
  },
  "dependencies": {
    "splunk-sdk": "^1.0.0",
    "commander": "^10.0.0",
    "log4js": "^6.9.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.11.0",
    "typescript-eslint": "^8.46.0",
    "dependency-cruiser": "^16.10.4",
    "eslint": "^9.37.0",
    "eslint-plugin-jsdoc": "^50.8.0",
    "eslint-plugin-sonarjs": "^3.0.5",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.3.0",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.0"
  }
}
